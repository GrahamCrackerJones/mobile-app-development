<?xml version="1.0" encoding="utf-8"?>
<manifest package="mobappdev.lecture23"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <permission android:name="mobappdev.lecture23.WHISPER" android:protectionLevel="signature"/>

    <uses-permission android:name="mobappdev.lecture23.WHISPER"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".talktomyself.TalkToMyselfActivity"/>
        <activity android:name=".whispershout.WhisperShoutActivity">
        </activity>

    </application>

</manifest>
